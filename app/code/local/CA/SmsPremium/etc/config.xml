            </smspremium_setup>
        </resources>
	</global>
	<admin>
        <routers>
			<smspremium>
				<use>admin</use>
				<args>
					<module>CA_Smspremium</module>
					<frontName>smspremium</frontName><!-- No utilizar admin, ya que este esta reservado para toda la parte de administraciÃ³n que trae Magento. -->
